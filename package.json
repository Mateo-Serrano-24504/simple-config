{
  "name": "@mateoserrano/simple-config",
  "version": "1.1.4",
  "type": "module",
  "private": false,
  "description": "Initialize ESLint, Prettier, Husky and lint-staged for modern TypeScript projects",
  "keywords": [
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "typescript",
    "cli"
  ],
  "files": [
    "scripts/",
    "templates/",
    "src/**"
  ],
  "bin": {
    "dev-config": "./scripts/init.js"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "25.0.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "tsx": "4.20.6",
    "typescript-eslint": "8.46.4"
  },
  "scripts": {
    "prepare": "husky"
  }
}
